# quick | normal | extended
TEST_LEVEL=extended

CPU_MINUTES=60
RAM_MINUTES=90

DISK_FIO_SIZE=10G
DISK_FIO_MINUTES=15

# Excluye el propio USB Ventoy (ejemplo: sda)
# Mira lsblk y pon aqu√≠ el disco del pendrive para no incluirlo en report/SMART si no quieres
EXCLUDE_DISKS=""

# No habilitado en este pack (no hay destructivo en auto). Mantener 0.
ALLOW_DESTRUCTIVE=1
AUTO_DESTRUCTIVE_CONFIRM=0

