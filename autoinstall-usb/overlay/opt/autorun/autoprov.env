# /opt/autorun/autoprov.env
# URL del bootstrap (usa *raw*, no la p√°gina HTML de GitHub)
GITHUB_BOOTSTRAP_URL="https://raw.githubusercontent.com/MarcPineiro/serverUtils/refs/heads/main/check-so/bootstrap.sh"

# Timeout total de espera de red (segundos)
NET_WAIT_SECONDS="25"

# Si 1, no vuelve a ejecutarse si ya existe el flag
RUN_ONCE="1"

# Donde guardar el flag (en /tce si existe)
RUN_ONCE_FLAG_REL="autoprov/ran.ok"

# Script local de fallback (incluido en el USB)
FALLBACK_SCRIPT="/opt/autorun/bootstrap.fallback.sh"

# Extra args opcionales para tu bootstrap
BOOTSTRAP_ARGS=""
